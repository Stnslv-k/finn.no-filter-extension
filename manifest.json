{
  "name": "Finn.no Filter and Data Extension",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.finn.no/*", "https://finn.no/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/js/distance.js", "/js/price.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage"]
}
